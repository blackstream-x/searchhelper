{
  "Application Title": "Debian Search Helper",
  "Description": "This program can be used for a quick search on web pages of the Debian project.\nAfter entering the search term and clicking on the 'Open' button or hitting the <Return> key, search results will open in new browser windows or tabs.\n",
  "Metadata": {
    "Author": "Rainer Schwarzbach",
    "Date": "2020-10-01",
    "Version": "0.8.0"
  },
  "Search URLs": {
    "Bugs": {
      "URL": "https://bugs.debian.org/{search_term}"
    },
    "Developer information": {
      "Preferred Browser": "my test browser",
      "URL": "https://tracker.debian.org/{search_term}"
    },
    "Packages": {
      "Preferred Browser": "chromium",
      "URL": "https://packages.debian.org/{search_term}"
    }
  },
  "Options": {
    "Mutually Exclusive Categories": false,
    "Register Browsers": {
      "my test browser": "/usr/bin/chromium"
    },
    "Special Select Keys": {
      "Control-Shift-A": {
        "Label": "all categories",
        "Categories": [
          "Bugs",
          "Developer information",
          "Packages"
        ]
      }
    }
  },
  "Translations": {
    "Search Term": "Search Term",
    "Clear Button": "Clear",
    "Open Button": "Open",
    "About Button": "About\u2026",
    "Quit Button": "Quit",
    "Copy URL": "Copy URL",
    "List URLs": "List URL names",
    "Program": "Program",
    "Config File": "Configuration file",
    "Special Key Selects": "The key combination {0} selects {1}.",
    "Category Error": "Fehler bei Kategorie {0!r}",
    "No Regex Match": "Search term {0!r} does not match the regular expression {1!r} and will be ignored.\n",
    "Regex Error": "Error in regular expression\n({0})\nPlease fix the error in the file {1}\n({2}) and restart this program!\n"
  }
}